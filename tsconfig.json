{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "declaration": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
        "*": ["node_modules/*", "src/*"],
        "@env": ["node_modules/react-native-dotenv"]
    },
    "jsx": "react",
  },
  "exclude": [
    "**/*.spec.ts",
    "node_modules",
    "dist",
  ],
  "compileOnSave": false,
  "typeRoots": ["./types"],
}