{
  "expo": {
    "name": "buttery-app",
    "slug": "buttery-app",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/splashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#2D90DD"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splashScreen.png",
        "backgroundColor": "#2D90DD"
      }
    },
    "description": "",
    "developmentClient": {
      "silentLaunch": true
    }
  }
}
