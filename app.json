{
  "expo": {
    "name": "Yale Butteries",
    "slug": "buttery-app",
    "platforms": ["ios", "android"],
    "version": "1.2.8",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/splashScreen.png",
      "resizeMode": "cover"
    },
    "backgroundColor": "#2D90DD",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/545b55a6-9a1a-4486-8846-88cecbc9c7dc"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.addisongoolsbee.butteryapp",
      "infoPlist": {
        "NSCameraUsageDescription": "Yale Butteries would like to access the camera to scan your card"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_transparent.png",
        "backgroundColor": "#2D90DD"
      },
      "package": "com.addisongoolsbee.butteryapp"
    },
    "description": "",
    "developmentClient": {
      "silentLaunch": true
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "extra": {
      "eas": {
        "projectId": "545b55a6-9a1a-4486-8846-88cecbc9c7dc"
      }
    },
    "plugins": ["expo-build-properties"]
  }
}
