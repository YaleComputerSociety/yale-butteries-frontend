{
  "expo": {
    "name": "Yale Butteries",
    "slug": "yale-butteries",
    "platforms": ["ios", "android"],
    "version": "1.2.15",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/splashScreen.png",
      "resizeMode": "cover"
    },
    "backgroundColor": "#2D90DD",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/de54dbe6-3119-4e4d-9b6d-dee822526dec"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.addisongoolsbee.butteryapp",
      "infoPlist": {
        "NSCameraUsageDescription": "Yale Butteries would like to access the camera to scan your card"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_transparent.png",
        "backgroundColor": "#2D90DD"
      },
      "package": "com.addisongoolsbee.butteryapp"
    },
    "description": "",
    "developmentClient": {
      "silentLaunch": true
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "entitlements": {
      "com.apple.developer.in-app-payments": ["merchant.com.yalebutteries"]
    },
    "extra": {
      "eas": {
        "projectId": "de54dbe6-3119-4e4d-9b6d-dee822526dec"
      }
    },
    "plugins": ["expo-build-properties"],
    "owner": "yale_butteries"
  }
}
